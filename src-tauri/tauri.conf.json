{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "unodrop",
  "version": "0.1.3",
  "identifier": "com.terry.unodrop",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1470",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Airdrop Manager",
        "width": 1440,
        "height": 900,
        "minWidth": 1280,
        "minHeight": 720,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}
